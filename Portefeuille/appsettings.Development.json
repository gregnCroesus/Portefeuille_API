{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "GeneralSettings": {
    "AllowedOrigins": [
      // Local Angular app
      "http://localhost:4200"
    ],

    // Max file size allowed for upload (in bytes)
    // 10 485 760 = 10MB
    "MultipartBodyLengthLimit": 10485760,

    "Log4NetConfigFile": "log4net.Development.config",

    "AuthServicesBaseURL": "http://localhost:5025/"
  }
}
